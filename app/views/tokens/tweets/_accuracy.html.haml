.row
  .col-xs-12
    .panel-body
      = render partial: 'layouts/menu'

.row
  .col-sm-12
    .panel.panel-default
      .panel-body.text-center.small
        .col-xs-1
          %h5= amount
          = model_name
        .col-xs-1
          %h5= token.metric.amount_rated
          = t('tokens.accuracy.rated')
        .col-xs-1
          %h5= token.metric.user_validate
          = t('tokens.accuracy.validade')
        .col-xs-1
          %h5= token.accuracy_human || '-'
          = t('tokens.accuracy.title')
