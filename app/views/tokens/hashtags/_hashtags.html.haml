.panel.panel-default.xs-padding
  .panel-body.xs-panel
    %br
    - if hashtags.any?
      %table.table
        %thead
          %tr
            %th
            %th.small.text-center= Token.human_attribute_name(:frequency)
        %tbody
          - hashtags.each do |hashtag|
            %tr
              %td= link_to hashtag.name, "https://twitter.com/hashtag/#{hashtag.human_name}", target: :blank
              %td.text-center= hashtag.df
