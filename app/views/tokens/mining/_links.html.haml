- token.top_links.each do |link|
  %ul.news
    %li
      %time.hidden-xs
        %span.day= link.df
        %span.month divulgações
      .info
        = link_to link.href, target: :blank do
          .title= raw link.human_title
        %ul
          %li{ style: 'width: 30%;' }
            .small= link.updated_at_to_human
          %li
